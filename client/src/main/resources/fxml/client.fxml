<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.HBox?>
<?import com.jfoenix.controls.JFXNodesList?>
<BorderPane prefHeight="540.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.chm.remote.client.controller.ClientDesktopController">
    <left>
        <GridPane BorderPane.alignment="CENTER">
            <columnConstraints>
                <ColumnConstraints halignment="RIGHT" maxWidth="40.0" minWidth="40.0" prefWidth="40.0"/>
                <ColumnConstraints halignment="CENTER" maxWidth="200.0" minWidth="10.0" prefWidth="200.0"/>
                <ColumnConstraints halignment="CENTER" maxWidth="10.0" minWidth="10.0" prefWidth="10.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="60.0" minHeight="10.0" prefHeight="50.0"/>
                <RowConstraints maxHeight="60.0" minHeight="10.0" prefHeight="50.0"/>
                <RowConstraints maxHeight="60.0" minHeight="10.0" prefHeight="50.0"/>
            </rowConstraints>
            <children>
                <Label fx:id="remoteIdLabel" textAlignment="CENTER" GridPane.rowIndex="1"/>
                <JFXTextField fx:id="remoteIdText" prefHeight="30.0" prefWidth="200.0" GridPane.columnIndex="1"
                              GridPane.rowIndex="1"/>
                <Label fx:id="passwordLabel" textAlignment="CENTER" GridPane.rowIndex="2"/>
                <JFXTextField fx:id="passwordText" prefHeight="30.0" prefWidth="200.0" GridPane.columnIndex="1"
                              GridPane.rowIndex="2"/>

                <Separator GridPane.rowSpan="3" GridPane.columnIndex="3" orientation="VERTICAL"/>
            </children>
        </GridPane>
    </left>
    <center>
        <JFXButton fx:id="btn" layoutX="113.0" layoutY="259.0" onAction="#btnClick" text="%Connection"/>
    </center>
</BorderPane>
